{
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://+:80"
      },
      "Https": {
        "Url": "https://+:443"
      }
    }
  },
  "ControllerUrl": "http://sail-controller.default.svc.cluster.local/api/dispatch",
  "ServiceName": "sail-proxy",
  "Jaeger": {
    "AgentHost": "localhost",
    "AgentPort": 6831,
    "Endpoint": "http://localhost:14268",
    "Protocol": "UdpCompactThrift"
  },
  "Certificate": {
    "DefaultPath": "/etc/ssl/certs/tls.crt",
    "DefaultKeyPath": "/etc/ssl/certs/tls.key"
  }
}